        <profile name="Barbed Embrace" typeId="7079-589c-df69-fa7e" typeName="Ability" hidden="false" id="ace5-dd31-4235-ec1f">
          <characteristics>
            <characteristic name="Description" typeId="4834-43a9-1c93-9062">An enemy model within 1&quot; of a Goetic Warlock cannot take a Retreat ACTION.</characteristic>
            <characteristic name="Range" typeId="2b32-2f45-9c06-7a77">-</characteristic>
            <characteristic name="Type" typeId="3f15-74a1-4d35-3642">Ability</characteristic>
