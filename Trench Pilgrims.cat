      <profiles>
        <profile name="Captive Giant" typeId="b3af-94f9-2e35-d1c5" typeName="Unit" hidden="false" id="822e-7e28-b8de-1cfc">
          <characteristics>
            <characteristic name="Movement" typeId="b725-e0a4-16ae-8df2">6&quot;/Infantry</characteristic>
