                        <characteristic name="Keywords" typeId="5093-5528-b08f-1496"/>
                        <characteristic name="Rules" typeId="4834-43a9-1c93-9062">The Children of Yggdrasil cannot field War Prophets, Communicants or Anchorite shrines, but instead can field Chieftains, Huscarls and Captive Giants respectively.</characteristic>
                      </characteristics>
                    </profile>
                    <profile name="Shieldwall" typeId="7079-589c-df69-fa7e" typeName="Battlekit" hidden="false" id="eb7d-c8c9-c9b8-c6b7">
                      <characteristics>
                        <characteristic name="Type" typeId="3f15-74a1-4d35-3642"/>
