                  <profiles>
                    <profile name="Warrior of the North" typeId="7079-589c-df69-fa7e" typeName="Battlekit" hidden="false" id="087c-68ea-c5da-cf0a">
                      <characteristics>
                        <characteristic name="Type" typeId="3f15-74a1-4d35-3642"/>
