        <selectionEntry type="upgrade" import="true" name="Explosive Charges" hidden="true" id="b535-89b0-70c0-a86f" sortIndex="13">
          <costs>
            <cost name="Ducats" typeId="8d43-33fd-332d-17a6" value="35"/>
