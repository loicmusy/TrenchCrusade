        <selectionEntry type="upgrade" import="true" name="Cartography &amp; Geometry" hidden="false" id="74b6-87eb-d383-ea59" sortIndex="2">
          <profiles>
            <profile name="Secrets of Cartography &amp; Geometry" typeId="7079-589c-df69-fa7e" typeName="Ability" hidden="false" id="4e71-6ded-f58e-8ffe">
              <characteristics>
                <characteristic name="Description" typeId="4834-43a9-1c93-9062">The Alchemist has studied long and hard the sciences of cartography and Geometry. At the start of the game, before deployment, you can pick up to 2 models in the Warband that are mounted on a base of 32mm or less, giving them the INFILTRATOR Keyword for that game.</characteristic>
                <characteristic name="Range" typeId="2b32-2f45-9c06-7a77"/>
                <characteristic name="Type" typeId="3f15-74a1-4d35-3642"/>
                <characteristic name="Keywords" typeId="5093-5528-b08f-1496"/>
              </characteristics>
            </profile>
          </profiles>
          <constraints>
            <constraint type="max" value="1" field="selections" scope="roster" shared="true" id="66d3-08bd-67f2-a932" includeChildSelections="true"/>
