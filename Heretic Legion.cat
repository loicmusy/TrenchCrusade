                <profile name="Enemies of All" typeId="7079-589c-df69-fa7e" typeName="Ability" hidden="false" id="dbc1-d8d1-1fed-a2c7">
                  <characteristics>
                    <characteristic name="Description" typeId="4834-43a9-1c93-9062">Trench Ghosts cannot use Mercenaries.</characteristic>
